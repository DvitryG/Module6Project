@page
@model Module6Project.Pages.AStarModel
@{
    ViewData["Title"] = "A*";
}
<div>
    <input id="digital" type="number" step="1" min="1" max="250">
    <button id="generate">Сгенерировать</button>
    <select id="colors">
        <option value="0">Ничего</option>
        <option value="1">Стены</option>
        <option value="2">Начало</option>
        <option value="3">Конец</option>
    </select>
    <button id="start">Начать!</button>
</div>
<canvas id="canvas"></canvas>
<script type="module" src="~/js/AStar.js" asp-append-version="true"></script>